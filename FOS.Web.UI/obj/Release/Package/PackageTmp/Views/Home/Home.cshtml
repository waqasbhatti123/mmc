<script>

     function GenerateDetailGrid() {
        debugger;


        $.ajax({
            type: "POST",
            dataType: 'json',
            url: '@Url.Action("GetAllData", "SaleOfficer")',
            contentType: 'application/json; charset=utf-8',

            success: function (json) {
                $("#tblbody").html("");
                debugger;
                var i=0;
                $.each(json, function (value, key) {
                    i++;
                    $("#datatab tbody").append("<tr ><td hidden='hidden'>" + key.ID + "</td><td>" + i + "</td><td>" + key.SoName +
                        "</td><td>" + key.SoName + "</td><td>" + key.CityName + "</td><td>" + key.Attstartstring + "</td><td>" + key.Attendstring + "</td><td>" + key.Locations + "</td><td>" + key.totalVisits + "</td><td>" + key.ProductiveShops + "</td><td>" + key.NonProductive + "</td><td>" + key.startstring + "</td><td>" + key.endstring + "</td><td>" + key.ElapseTime + "</td><td>" + key.totalSale + "</td></tr>");

                });
            }
        });
    }


