<!-- Modal -->
<div id="ChooseDates1" class="modal fade" role="dialog" style="width: 45%;left: 29%;height: 30%;">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">

            <div class="modal-header" style="background-color: #3F51B5;box-shadow: 0px 3px 8px grey;">
                <button type="button" class="close" data-dismiss="modal" style="margin-top: -8px;color: #ffffff;opacity: 0.8;">&times;</button>
                <h4 class="modal-title" style="font-weight: 900;color: #ffffff;margin-top: 0px;margin-bottom: 0px;">FOS Wise Order Intake/Delivered Report</h4>
            </div>

            <div class="modal-body">

                <div class="span12" style="margin-left: 35px;">
                    <div class="span6">

                        <div class="control-group" style="margin-bottom: 14px;">
                            <label class="control-label" style="margin-left: 25px;">Tick for date wise</label>
                            <div class="controls" style="margin-top: -28px;">
                                <input type="checkbox" id="cbCheck1" name="cbCheck1" ">
                            </div>
                        </div>

                        <div class="control-group" style="margin-bottom: 14px;">
                            <label class="control-label">Starting</label>
                            <div class="controls">
                                <input class="text-box single-line" id="StartingDate1" name="StartingDate1" style="height: 22px; width: 202px; margin-bottom: 0px;">
                            </div>
                        </div>
                    </div>

                    <div class="span6" style="margin-top: 30px;">
                        <div class="control-group" style="margin-bottom: 14px;">
                            <label class="control-label">Ending</label>
                            <div class="controls">
                                <input class="text-box single-line" id="EndingDate1" name="EndingDate1" style="height: 22px; width: 202px; margin-bottom: 0px;">
                            </div>
                        </div>

                        <button type="button" id="GenerateReport1" class="btn btn-primary" style="float: right;margin-right: 70px;">
                            <span id="create">Generate Report</span>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

